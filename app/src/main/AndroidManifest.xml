<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jinke.driverhealth">
    <!-- 网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!--通讯录-->
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />

    <application
        android:name=".DHapplication"
        android:allowBackup="true"
        android:icon="@mipmap/app_icon180"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DriverHealth">
        <activity android:name=".activity.ContacterActivity"></activity>
        <activity android:name=".activity.AlcoholActivity" />
        <activity android:name=".activity.BloodPressureActivity" />
        <activity android:name=".activity.TempActivity" />
        <activity android:name=".activity.HeartActivity" />
        <activity android:name=".activity.HealthDetailActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>